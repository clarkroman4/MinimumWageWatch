
@model FinalProject.ViewModels.ViewLocationsViewModel
@{
    ViewData["Title"] = "All Locations";
}

<h1>All Locations</h1>
<table class="table-striped">
    <tr>
        <th>Minimum Wage</th>
        <th>Location Name</th>
        <th>Address</th>
        <th>City</th>
        <th>County</th>
        <th>State</th>
        <th>ZIP</th>
    </tr>

    @foreach(Location location in Model.Locations) {
    <tr>
        <td>@Model.Wage</td>
        <td>@location.Name</td>
        <td> @location.Address</td>
        <td>@location.City</td>
        <td>@location.County</td>
        <td>@location.State</td>
        <td>@location.ZIP</td>
    </tr>
    }
</table>